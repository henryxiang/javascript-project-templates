{
  "plugins": ["node", "jest"],
  "extends": ["eslint:recommended", "plugin:node/recommended"],
  "rules": {
    "node/no-unsupported-features": 0,
    "no-console": 0
  },
  "env": {
    "jest/globals": true,
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  }
}