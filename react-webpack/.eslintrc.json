{
  "extends": [
    "plugin:react/recommended",
    "airbnb",
    "plugin:prettier/recommended",
    "prettier/react"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-console": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "react/jsx-filename-extension": 0
  },
  "parser": "babel-eslint",
  "globals": {
    "document": false
  }
}
